    },
    "cSpell.language": ",es",
    "cSpell.enabled": false,
    // Controls the font family.
    "editor.fontFamily": "OperatorMono-Book, Fira Code, Victor Mono, Menlo, Monaco, 'Courier New', monospace",
    // Enables/Disables font ligatures.
    "editor.fontLigatures": true,
    // Controls the font size in pixels.
    "workbench.colorTheme": "Operator Mono Dark Theme",
    // Controls the font weight.
    "editor.fontWeight": "400", //Normal = 400
    // Latex-Tools Compilers
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk üîÉ",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "latexmk (latexmkrc)",
            "tools": [
                "latexmk_rconly"
            ]
        },
        {
            "name": "latexmk (lualatex)",
            "tools": [
                "lualatexmk"
            ]
        },
        {
            "name": "pdflatex ‚ûû bibtex ‚ûû pdflatex‚Ää√ó‚Ää2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "xelatexmk (xelatex)",
            "tools": [
                "xelatexmk"
            ]
        }
    ],
    "workbench.settings.useSplitJSON": true,
    // Prettier
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.singleQuote": true,
    "prettier.jsxSingleQuote": true,
    // ESLint
    "editor.codeActionsOnSave": {
        // For ESLint
        "source.fixAll.eslint": true,
        // For TSLint
        "source.fixAll.tslint": true,
        // For Stylelint
        "source.fixAll.stylelint": true
    },
    // VSCode
    "editor.formatOnSave": true,
    // Vetur
    "vetur.validation.template": false,
    "vetur.format.defaultFormatterOptions": {
        "prettyhtml": {
          "printWidth": 100, // No line exceeds 100 characters
          "singleQuote": false // Prefer double quotes over single quotes
        }
    },
    "search.useIgnoreFiles": false,
    "editor.largeFileOptimizations": false,
    "editor.dragAndDrop": false,
    "workbench.startupEditor": "newUntitledFile",
    "editor.tabSize": 2
}