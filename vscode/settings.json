  "cSpell.ignoreRegExpList": [
    "\\\\cite{[A-Za-z0-9, -]+}",
    "\\\\begin{\\w+}",
    "\\\\end{\\w+}",
    "\\\\usepackage{\\w+}",
    "\\\\bibliographystyle{\\w+}",
    "\\\\hyphenation{[A-Za-z0-9, -]+}",
    "\\w+{",
    "\\\\textwidth",
    "itemizecolor"
  ],
  "rewrap.wrappingColumn": 100,
  "cSpell.language": ",es",
  "cSpell.enabled": false,
  // Controls the font family.
  "editor.fontFamily": "OperatorMono-Book, Fira Code, Victor Mono, Menlo, Monaco, 'Courier New', monospace",
  // Enables/Disables font ligatures.
  "editor.fontLigatures": true,
  // Controls the font size in pixels.
  "workbench.colorTheme": "Operator Mono Dark Theme",
  // Controls the font weight.
  "editor.fontWeight": "400", //Normal = 400
  // Latex-Tools Compilers
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "lualatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "latexmk_rconly",
      "command": "latexmk",
      "args": ["%DOC%"],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "env": {}
    },
    {
      "name": "xelatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-xelatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk üîÉ",
      "tools": ["latexmk"]
    },
    {
      "name": "latexmk (latexmkrc)",
      "tools": ["latexmk_rconly"]
    },
    {
      "name": "latexmk (lualatex)",
      "tools": ["lualatexmk"]
    },
    {
      "name": "pdflatex ‚ûû bibtex ‚ûû pdflatex‚Ää√ó‚Ää2",
      "tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
    },
    {
      "name": "xelatexmk (xelatex)",
      "tools": ["xelatexmk"]
    }
  ],
  "workbench.settings.useSplitJSON": true,

  "search.useIgnoreFiles": false,
  "editor.largeFileOptimizations": false,
  "editor.dragAndDrop": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,

  // SideBar Location
  "workbench.sideBar.location": "left",

  // Vim Settings
  "vim.textwidth": 100,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "vim.camelCaseMotion.enable": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["<C-`>"],
      "after": ["<Esc>"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-`>"],
      "after": ["<Esc>"]
    }
  ],
  "vim.neovimPath": "/usr/local/bin/nvim",

  //   VUE + JAVASCRIPT
  //***************************

  // Eslint configuration
  // https://eslint.vuejs.org/user-guide/#faq
  // Vetur
  "vetur.validation.template": false,
  "eslint.validate": ["javascript", "vue", "html"],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },

  // General formatting
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.singleQuote": true,

  //   PYTHON
  //***************************

  // Python formatting
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "python.formatting.provider": "black",
  // // Path to Black, you can use a custom version of Black by modifying this setting to include the full path.
  // "python.formatting.blackPath": "/Users/fer/.pyenv/versions/3.8.5/bin/black",
  // Python Linting: Disable pylint, enable flake 8
  "python.sortImports.args": ["--profile=black"]
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.linting.flake8Path": "/Users/fer/.local/pipx/venvs/flake8/bin/flake8",
  // Python interpreter
  "python.defaultInterpreterPath": "/Users/fer/miniconda3/envs/dsfull/bin/python",
"window.zoomLevel": 1,
"jupyter.interactiveWindow.textEditor.executeSelection": false
}
